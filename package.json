{
  "name": "openheroselect",
  "version": "4.0.1",
  "description": "Open Source HeroSelect for Marvel Ultimate Alliance (2006)",
  "main": "index.js",
  "scripts": {
    "pipsetup": "pip install auto-py-to-exe",
    "clean": "npm i && node ./_build.js -c",
    "fullclean": "npm i && node ./_build.js -f",
    "build": "node ./_build.js",
    "distpackage": "npm run fullclean && npm run build && node ./_build.js -p",
    "buildohs": "node ./_build.js -t ohs",
    "buildxml2json": "node ./_build.js -t xml2json",
    "buildjson2xmlb": "node ./_build.js -t json2xmlb",
    "buildcopyfiles": "node ./_build.js -t copyfiles"
  },
  "bin": {
    "index": "./index.js"
  },
  "keywords": [],
  "author": "Tony Stark @ MarvelMods (TheRealPSV @ GitHub)",
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "enquirer": "^2.3.6",
    "fs-extra": "^10.0.0",
    "minimist": "^1.2.5",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "7zip-bin": "^5.1.1",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "glob": "^7.1.7",
    "node-7z": "^2.1.2",
    "pkg": "^5.3.0",
    "rcedit": "^3.0.0"
  }
}
